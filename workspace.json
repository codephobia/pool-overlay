{
    "version": 1,
    "projects": {
        "api": {
            "projectType": "application",
            "root": "apps/api",
            "sourceRoot": "apps/api",
            "architect": {
                "build": {
                    "builder": "@nx-go/nx-go:build",
                    "options": {
                        "outputPath": "dist/apps/api/api",
                        "main": "./apps/api"
                    }
                },
                "serve": {
                    "builder": "@nx-go/nx-go:serve",
                    "options": {
                        "cwd": "apps/api",
                        "main": "."
                    }
                },
                "test": {
                    "builder": "@nx-go/nx-go:test",
                    "options": {
                        "main": "./apps/api"
                    }
                },
                "lint": {
                    "builder": "@nx-go/nx-go:lint"
                }
            },
            "tags": []
        },
        "dashboard": {
            "projectType": "application",
            "root": "apps/dashboard",
            "sourceRoot": "apps/dashboard/src",
            "prefix": "pool-overlay",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/dashboard",
                        "index": "apps/dashboard/src/index.html",
                        "main": "apps/dashboard/src/main.ts",
                        "polyfills": "apps/dashboard/src/polyfills.ts",
                        "tsConfig": "apps/dashboard/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "apps/dashboard/src/favicon.ico",
                            "apps/dashboard/src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                            "apps/dashboard/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "apps/dashboard/src/environments/environment.ts",
                                    "with": "apps/dashboard/src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "dashboard:build:production"
                        },
                        "development": {
                            "browserTarget": "dashboard:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "dashboard:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/dashboard/src/**/*.ts",
                            "apps/dashboard/src/**/*.html"
                        ]
                    },
                    "outputs": ["{options.outputFile}"]
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/dashboard"],
                    "options": {
                        "jestConfig": "apps/dashboard/jest.config.ts",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
        "go": {
            "projectType": "library",
            "root": "libs/go",
            "sourceRoot": "libs/go",
            "architect": {
                "test": {
                    "builder": "@nx-go/nx-go:test"
                },
                "lint": {
                    "builder": "@nx-go/nx-go:lint"
                }
            },
            "tags": []
        },
        "models": {
            "projectType": "library",
            "root": "libs/models",
            "sourceRoot": "libs/models/src",
            "prefix": "pool-overlay",
            "architect": {
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/models"],
                    "options": {
                        "jestConfig": "libs/models/jest.config.ts",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/models/src/**/*.ts",
                            "libs/models/src/**/*.html"
                        ]
                    },
                    "outputs": ["{options.outputFile}"]
                }
            },
            "tags": []
        },
        "overlay": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/overlay",
            "sourceRoot": "apps/overlay/src",
            "prefix": "pool-overlay",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/overlay",
                        "index": "apps/overlay/src/index.html",
                        "main": "apps/overlay/src/main.ts",
                        "polyfills": "apps/overlay/src/polyfills.ts",
                        "tsConfig": "apps/overlay/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/overlay/src/favicon.ico",
                            "apps/overlay/src/assets"
                        ],
                        "styles": ["apps/overlay/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/overlay/src/environments/environment.ts",
                                    "with": "apps/overlay/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    },
                    "outputs": ["{options.outputPath}"]
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "overlay:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "overlay:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "overlay:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/overlay/tsconfig.app.json",
                            "apps/overlay/tsconfig.spec.json",
                            "apps/overlay/tsconfig.editor.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/overlay/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/overlay/jest.config.ts",
                        "passWithNoTests": true
                    },
                    "outputs": ["coverage/apps/overlay"]
                }
            },
            "tags": []
        },
        "scoreboard": {
            "projectType": "library",
            "root": "libs/scoreboard",
            "sourceRoot": "libs/scoreboard/src",
            "prefix": "pool-overlay",
            "architect": {
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/scoreboard"],
                    "options": {
                        "jestConfig": "libs/scoreboard/jest.config.ts",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/scoreboard/src/**/*.ts",
                            "libs/scoreboard/src/**/*.html"
                        ]
                    },
                    "outputs": ["{options.outputFile}"]
                }
            },
            "tags": []
        },
        "seed": {
            "projectType": "application",
            "root": "apps/seed",
            "sourceRoot": "apps/seed",
            "architect": {
                "build": {
                    "builder": "@nx-go/nx-go:build",
                    "options": {
                        "outputPath": "dist/apps/seed/seed",
                        "main": "./apps/seed"
                    }
                },
                "serve": {
                    "builder": "@nx-go/nx-go:serve",
                    "options": {
                        "cwd": "./apps/seed",
                        "main": "."
                    }
                },
                "test": {
                    "builder": "@nx-go/nx-go:test"
                },
                "lint": {
                    "builder": "@nx-go/nx-go:lint"
                }
            },
            "tags": []
        },
        "telestrator": {
            "projectType": "application",
            "root": "apps/telestrator",
            "sourceRoot": "apps/telestrator/src",
            "prefix": "pool-overlay",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/telestrator",
                        "index": "apps/telestrator/src/index.html",
                        "main": "apps/telestrator/src/main.ts",
                        "polyfills": "apps/telestrator/src/polyfills.ts",
                        "tsConfig": "apps/telestrator/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "apps/telestrator/src/favicon.ico",
                            "apps/telestrator/src/assets"
                        ],
                        "styles": ["apps/telestrator/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "apps/telestrator/src/environments/environment.ts",
                                    "with": "apps/telestrator/src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "telestrator:build:production"
                        },
                        "development": {
                            "browserTarget": "telestrator:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "telestrator:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/telestrator/src/**/*.ts",
                            "apps/telestrator/src/**/*.html"
                        ]
                    },
                    "outputs": ["{options.outputFile}"]
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/telestrator"],
                    "options": {
                        "jestConfig": "apps/telestrator/jest.config.ts",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        }
    }
}
